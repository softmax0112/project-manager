<p id="notice"><%= notice %></p>

<h1>Time Logs</h1>

<table>
  <thead>
    <tr>
      <th>Project</th>
      <th>User</th>
      <th>Hours</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @time_logs.each do |time_log| %>
      <tr>
        <td><%= Project.find(time_log.project_id).title %></td>
        <td><%= User.find(time_log.user_id).name %></td>
        <td><%= time_log.hours %></td>
        <td><%= link_to 'Show', time_log %></td>
        <td><%= link_to 'Edit', edit_time_log_path(time_log) %></td>
        <td><%= link_to 'Destroy', time_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Time Log', new_time_log_path %>
